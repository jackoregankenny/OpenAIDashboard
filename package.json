{
  "name": "openai-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate:registry && next dev --turbo",
    "dev:watch": "concurrently \"npm run dev\" \"npm run watch:registry\"",
    "build": "npm run generate:registry && next build",
    "start": "next start",
    "lint": "next lint",
    "generate:registry": "tsx src/scripts/generate-registry.ts",
    "watch:registry": "chokidar 'src/components/widgets/**/widget.config.ts' -c 'npm run generate:registry'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.16.3",
    "@tanstack/react-query": "^5.90.2",
    "chokidar-cli": "^3.0.0",
    "chroma-js": "^3.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "glob": "^11.0.3",
    "lucide-react": "^0.544.0",
    "next": "^15.5.0",
    "node-vibrant": "^4.0.3",
    "prisma": "^6.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/chroma-js": "^3.1.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "^15.5.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
